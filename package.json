    "sass": "1.62.1",
    "tslib": "^2.6.2",
    "typescript": "5.5.4",
    "vite": "~5.0.0",
    "vite-plugin-dts": "~2.3.0",
    "vitepress": "^1.0.1",
    "vitest": "1.6.0",
    "vue-tsc": "2.0.7"
