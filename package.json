    "@commitlint/cz-commitlint": "^19.2.0",
    "@nx/eslint": "19.6.3",
    "@nx/js": "19.6.3",
    "@nx/rollup": "19.6.3",
    "@nx/vite": "19.6.3",
    "@nx/vue": "19.6.3",
    "@nx/web": "19.6.3",
    "@swc-node/register": "1.9.2",
    "@swc/cli": "0.3.14",
    "@swc/core": "1.5.7",
    "@swc/helpers": "0.5.12",
    "@types/node": "18.16.9",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/ui": "1.6.0",
    "@vue/test-utils": "^2.4.1",
    "commitizen": "^4.3.0",
